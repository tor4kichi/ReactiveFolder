<UserControl x:Class="Modules.Main.Views.ReactionEditerPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Modules.Main.Views"
             xmlns:prism="http://prismlibrary.com/"             
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>

        <Grid x:Name="Menu">
            <Button Content="Back" Width="50" Command="{Binding BackCommand}" HorizontalAlignment="Left"/>
            <Button Content="Test" Width="50" Command="{Binding BackCommand}" HorizontalAlignment="Right"/>
        </Grid>

        <Grid x:Name="ReactionContent" Grid.Row="1">
            <TextBlock Text="{Binding Name.Value}" Grid.RowSpan="2" />

            <StackPanel>
                <TextBlock Text="Filter" />

            </StackPanel>
        </Grid>
    </Grid>
</UserControl>
